# create a MDI type for email addresses; see: https://www.regular-expressions.info/email.html

name="email_14"
description="Email address (RFC 5322)"
type=Regex
regex='^[a-z0-9!#$%&'"'"'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'"'"'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?$'
isSensitive=false
isRequired=false
